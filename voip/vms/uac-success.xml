<!-- Duracion de llamadas												-->
<!-- 					MIN			MAX									-->	
<!-- 		Ringing		 2000		 5000								-->
<!-- 		ACK - BYE	 5000		10000								-->
<!-- 		timewait	 5000		 5000								-->
<!-- 		TOTAL		12000		20000								-->
<!--                                                                    -->
<!--                         LIMITE                                     -->
<!--       rate 10/s    120         200                                 -->
<!--                                                                    -->
<!--                                                                    -->


<scenario name="Basic Sipstone UAC">
  <!-- In client mode (sipp placing calls), the Call-ID MUST be         -->
  <!-- generated by sipp. To do so, use [call_id] keyword.                -->
  <send start_txn="invite" retrans="500" start_rtd="invite">
    <![CDATA[

      INVITE sip:[field0]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: 123456 <sip:123456@[local_ip]:[local_port]>;tag=[pid]SIPpTag00[call_number]
      To: [field0] <sip:[field0]@[remote_ip]:[remote_port]>
      Call-ID: [call_id]
      CSeq: 1 INVITE
      Contact: sip:123456@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Type: application/sdp
      Content-Length: [len]

      v=0
      o=user1 53655765 2353687637 IN IP[local_ip_type] [local_ip]
      s=-
      c=IN IP[media_ip_type] [media_ip]
      t=0 0
      m=audio [media_port] RTP/AVP 0
      a=rtpmap:0 PCMU/8000

    ]]>
  </send>

  <!-- Para evitar unexpected msj se ignora el mansaje -->
  <recv response="100"
        optional="true"
		response_txn="invite"
		lost="100">
  </recv>

  <recv response="180"
        optional="true"
        response_txn="invite"
		rtd="invite"
		crlf="true">
  </recv>


  <!-- By adding rrs="true" (Record Route Sets), the route sets         -->
  <!-- are saved and used for following messages sent. Useful to test   -->
  <!-- against stateful SIP proxies/B2BUAs.                             -->
  <recv response="200" 
        response_txn="invite"
		rrs="true">
  </recv>

  <!-- Packet lost can be simulated in any send/recv message by         -->
  <!-- by adding the 'lost = "10"'. Value can be [1-100] percent.       -->
  <send ack_txn="invite" crlf="true">
    <![CDATA[

      ACK [next_url] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      [routes]
      From: 123456 <sip:123456@[local_ip]:[local_port]>;tag=[pid]SIPpTag00[call_number]
      To: [field0] <sip:[field0]@[remote_ip]:[remote_port]>[peer_tag_param]
      Call-ID: [call_id]
      CSeq: 1 ACK
      Contact: sip:123456@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>

  <!-- This delay can be customized by the -d command-line option       -->
  <!-- or by adding a 'milliseconds = "value"' option here.             -->
  <pause distribution="uniform" min="5000" max="10000" crlf="true"/>

  <!-- The 'crlf' option inserts a blank line in the statistics report. -->
  <send start_txn="bye" retrans="500" start_rtd="bye">
    <![CDATA[

      BYE [next_url] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      [routes]
      From: 123456 <sip:123456@[local_ip]:[local_port]>;tag=[pid]SIPpTag00[call_number]
      To: [field0] <sip:[field0]@[remote_ip]:[remote_port]>[peer_tag_param]
      Call-ID: [call_id]
      CSeq: 2 BYE
      Contact: sip:123456@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>

  <recv response="200" 
        response_txn="bye" 
		rtd="bye"
		crlf="true">
  </recv>
  
  <!-- En caso que sillgan llegando 200 OKs     -->
  <timewait milliseconds="5000"/>

  <!-- definition of the response time repartition table (unit is ms)   -->
  <ResponseTimeRepartition value="10, 20, 30, 40, 50, 100, 150, 200, 400, 800"/>

  <!-- definition of the call length repartition table (unit is ms)     -->
  <CallLengthRepartition value="10000, 14000, 18000, 22000, 26000, 30000"/>

</scenario>

